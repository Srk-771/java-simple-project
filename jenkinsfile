job('Pipeline_Job'){
    description('first job as infrastructure as a code')
    
    scm ('git clone'){
        url: 'https://github.com/Srk-771/test-project-1.git',master 
    }
    triggers ('polscm'){
    	scm('* * * * *')
    }

    stage('mvn'){
        maven ('pom.xml','clean package')
    }
   
}
